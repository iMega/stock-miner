type Query {
    user: User!
    stockItemApproved: [StockItem]
}

type Mutation {
    addStockItemApproved(item: StockItemInput!): StockItem!
}

type User {
    email: ID!
    name: String
    avatar: String
}

type StockItem {
    ticker: String!
    figi: String!
    amountLimit: Float!
    transactionLimit: Int!
}

input StockItemInput {
    ticker: String!
    figi: String!
    amountLimit: Float!
    transactionLimit: Int!
}
